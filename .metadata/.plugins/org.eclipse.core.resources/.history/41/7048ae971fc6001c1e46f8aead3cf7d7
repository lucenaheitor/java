package rooms;

import java.util.Scanner;

public class Rooms {

	public static void main(String[] args) {
		
		
		Scanner tc = new Scanner(System.in);
		
		Rent[] room = new Rent[10];
		
		
		System.out.print(" How many rooms will be rented? ");
		int rented = tc.nextInt();
		
		for( int i=1; i<= rented; i++) {
			System.out.println("Rent#" + i + ": ");
			
			System.out.println("Name: ");
			String name = tc.nextLine();
			room[i-1].setName(name);
			System.out.println("Email: ");
			String email = tc.nextLine();
			room[i-1].setEmail(email);
			System.out.println("Room: ");
			String  quarto = tc.nextLine();
			room[i-1].setRoom(quarto);
				
		}
		
		for( int i=0; i<rented;i++) {
			
			
		}
		
		
		

	}

}
