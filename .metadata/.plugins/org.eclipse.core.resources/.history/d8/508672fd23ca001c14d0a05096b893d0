package matriz;

import java.util.Scanner;

public class Matriz {

	public static void main(String[] args) {
		 Scanner tc = new Scanner(System.in);
		 
		 int n  = tc.nextInt();
		 int [][] mat = new int [n][n];
		 
		 for( int i =0; i<n; i++) {
			 for(int j=0; j<n; j++) {
				 mat[i][j] = tc.nextInt();
				 
			 }
		 }
		 
		 System.out.println("Main diagonal: ");
		 for (int i=0; i<n; i++) {
			 System.out.print(mat[i][i] + " ");
		 }
		 
		 
		 tc.close();

	}

}
